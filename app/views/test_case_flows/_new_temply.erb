<%if @test_case_flow_new != nil %>
 <div>
    <%= form_for(@test_case_flow_new, :remote => true) do |f| %>
    <%= f.text_field :test_case_id,:type=>'hidden' %>
    <%= f.number_field :flow_seq,:readonly=>'readonly' %>
    <%= f.select :flow_type_id, FlowType.find(:all).collect {|p| [ p.name, p.id ] } %>
    <%= f.text_field :flow_location,:id=>"test_case_flow_flow_location_#{@test_case_flow_new.flow_seq}"%>
    <%= f.select :flow_date_type_id, FlowDateType.find(:all).collect {|p| [ p.name, p.id ] } %>
    <%= f.text_area :flow_date,:rows=>1,:style=>"margin-top:6px;margin-bottom:-6px" ,:id=>"test_case_flow_flow_date_#{@test_case_flow_new.flow_seq}" %>
    <%= f.submit :保存 %>
    <% end %>
 </div>
   <% end %>